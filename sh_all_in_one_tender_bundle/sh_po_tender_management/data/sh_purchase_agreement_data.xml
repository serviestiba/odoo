<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_purchase_agreement_seq" model="ir.sequence">
        <field name="name">Purchase Agreement Sequence</field>
        <field name="code">purchase.agreement</field>
        <field name="prefix">TE</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="cron_auto_close_tender" model="ir.cron">
        <field name="name">Auto Close Tender</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="model_id" ref="sh_all_in_one_tender_bundle.model_purchase_agreement" />
        <field name="code">model._run_auto_close_tender()</field>
        <field name="state">code</field>
    </record>
</odoo>
