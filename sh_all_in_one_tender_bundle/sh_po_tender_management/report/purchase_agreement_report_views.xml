<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Purchase Agreement Report Print Action -->
    <record id="action_report_purchase_tender" model="ir.actions.report">
        <field name="name">Purchase Tender</field>
        <field name="model">purchase.agreement</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sh_all_in_one_tender_bundle.report_purchasetender</field>
        <field name="report_file">sh_all_in_one_tender_bundle.report_purchasetender</field>
        <field name="binding_model_id" ref="model_purchase_agreement" />
        <field name="binding_type">report</field>
    </record>
    <!-- Analyze Quotations Report Print Action -->
    <record id="action_report_analyze_quotations" model="ir.actions.report">
        <field name="name">Analyze Quotations</field>
        <field name="model">purchase.agreement</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">sh_all_in_one_tender_bundle.report_analyze_quotations</field>
        <field name="report_file">sh_all_in_one_tender_bundle.report_analyze_quotations</field>
        <field name="binding_model_id" ref="model_purchase_agreement" />
        <field name="binding_type">report</field>
    </record>
</odoo>
