<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="serviestiba_stock_view_picking_form" model="ir.ui.view">
		<field name="name">serviestiba.stock.view.picking.form</field>
		<field name="model">stock.picking</field>
		<field name="inherit_id" ref="stock.view_picking_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='scheduled_date']" position="replace">
				<field name="scheduled_date" readonly="1"/>
			</xpath>
		</field>
	</record>
</odoo>